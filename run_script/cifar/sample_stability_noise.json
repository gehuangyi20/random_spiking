{
    "g_options": [
       "python3 RsNet/rand_dir_inference.py",
        "--gpu_idx=0",
        "--is_logits=no",
        "--data_dir=cifar_data",
        "--data_name=data",
        "--model_dir=result_cifar/models_sample",
        "--batch_size=32",
        "--set_name=cifar10",
        "--data_format=channels_first",
        "--noise_iter=20 --test_data_start=0 --test_data_len=100"
    ],
    "task":[
        {
            "name": "noise",
            "options": [],
            "iter_options": ["0", "0.5", "1", "1.5", "2", "2.5"]
        }
    ],
    "sub_task":[
        {
            "name": "standard",
            "model":[{
                "prefix": "--model_name=",
                "format": "cifar_wrn_28_10_standard_%d",
                "arg": ["d"]
            }],
            "model_len": 4,
            "options": ["--iteration=1", "--dropout=0", "--rspike_layer=0", "--bagging=False"],
            "iter": [
                {
                    "format": "--noise_distance=%s --output_file=result_cifar/models_noise_sample/standard/%s.csv",
                    "arg": ["o", "o"]
                }
            ]
        },
        {
            "name": "rs1_nodropout",
            "model":[{
                "prefix": "--model_name=",
                "format": "cifar_wrn_28_10_rs1_nodropout_%d",
                "arg": ["d"]
            }],
            "model_len": 4,
            "options": ["--iteration=10", "--dropout=0", "--rspike_layer=1", "--bagging=True"],
            "iter": [
                {
                    "format": "--noise_distance=%s --output_file=result_cifar/models_noise_sample/rs1_nodropout/%s.csv",
                    "arg": ["o", "o"]
                }
            ]
        }
    ]
}
